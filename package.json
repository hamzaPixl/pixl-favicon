    "react-query": "^3.39.3",
    "react-type-animation": "^3.0.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "eslint": "8.47.0",
