    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/node": "20.5.4",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "axios": "^1.4.0",
    "babel-plugin-formatjs": "^10.5.3",
    "cookies-next": "^3.0.0",
    "eslint-plugin-formatjs": "^4.10.3",
    "flowbite": "^1.7.0",
    "flowbite-react": "^0.5.0",
