    "i18n": "npm run extract:i18n && npm run compile:i18n",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "dependencies": {
    "@formatjs/cli": "^6.1.3",
